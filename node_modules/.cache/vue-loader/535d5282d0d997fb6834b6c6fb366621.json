to=\"product.get_absolute_url\">\n                  <span aria-hidden=\"true\" class=\"absolute inset-0\" />\n                  {{ product.name }}\n                </router-link>\n              </h2></strong>\n              \n              <p class=\"mt-1 text-sm  font-medium text-black-500\">Monthly Rent: Rs.{{ product.rent }}</p>\n              <p class=\"text-sm font-medium text-gray-900\">Deposit: Rs.{{ product.deposit }}</p>\n\n            </div>\n             <p class=\"text-sm font-medium text-gray-900\">MRP: Rs.{{ product.mrp }}</p>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport axios from 'axios'\n\nexport default {\n    name: 'Category',\n\n    data() {\n        return {\n            category: {\n                products: []\n            }\n        }\n    },\n    mounted() {\n        this.getCategory()\n    },\n    watch: {\n        $route(to, from) {\n            if (to.name === 'Category') {\n                this.getCategory()\n            }\n        }\n    },\n    methods: {\n        async getCategory() {\n            const categorySlug = this.$route.params.category_slug\n            axios\n                .get(`/api/v1/books/${categorySlug}/`)\n                .then(response => {\n                    this.category = response.data\n                    document.title = this.category.name + ' | KitabKuber'\n                })\n                .catch(error => {\n                    console.log(error)\n\n                })\n\n        }\n    }\n}\n</script>"]}]}